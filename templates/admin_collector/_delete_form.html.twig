<form method="post" action="{{ path('admin_collector_delete', {'id': collector.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ collector.id) }}">
	<button class="btn btn-primary fs-5 text-light px-3 text-center mx-3">Supprimer</button>
</form>
